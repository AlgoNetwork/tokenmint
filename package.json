{
  "name": "tokenmint",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://tokenmint.io/mint",
  "dependencies": {
    "@babel/runtime": "^7.1.2",
    "@material-ui/core": "^3.1.2",
    "cryptocompare": "^0.5.0",
    "openzeppelin-solidity": "1.12.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "react-scripts": "1.1.5",
    "redux": "^4.0.0",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-thunk": "^2.3.0",
    "web3": "^1.0.0-beta.36"
  },
  "scripts": {
    "ganache": "ganache-cli -p 7545",
    "compile-contracts": "truffle.cmd compile",
    "deploy-contracts-local": "truffle.cmd migrate --reset --network local",
    "prestart": "npm-run-all print:start:message",
    "start": "npm-run-all --parallel lint:watch test:watch react:start",
    "build": "react-scripts build",
    "deploy": "npm-run-all build copy:build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "print:start:message": "babel-node src/tools/startMessage.js",
    "lint:watch": "npm run lint -- --watch",
    "lint": "esw src --color",
    "react:start": "react-scripts start",
    "copy:build": "babel-node src/tools/copyBuild",
    "test:watch": "npm run test -- --watch"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "chalk": "^2.4.1",
    "eslint": "^5.6.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-watch": "^4.0.2",
    "fs-extra": "^7.0.0",
    "ganache-cli": "6.1.0",
    "npm-run-all": "^4.1.3"
  }
}
